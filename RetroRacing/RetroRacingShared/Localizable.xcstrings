    "settings_controls_ios": {
      "extractionState": "manual",
      "localizations": {
        "ca": {
          "stringUnit": {
            "state": "translated",
            "value": "Llisca cap a l'esquerra o la dreta per moure.\nToca la meitat esquerra o dreta de la pantalla.\nAmb VoiceOver: toca les zones esquerra o dreta i doble toc per moure.\nFes servir les tecles de fletxa esquerra i dreta amb un teclat connectat."
          }
        },
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Swipe left or right to move.\nTap the left or right half of the screen.\nWith VoiceOver: use the left and right touch areas and double-tap to move.\nUse the left and right arrow keys when a keyboard is connected."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Desliza a la izquierda o derecha para mover.\nToca la mitad izquierda o derecha de la pantalla.\nCon VoiceOver: usa las zonas táctiles izquierda y derecha y doble toque para mover.\nUsa las teclas de flecha con un teclado conectado."
          }
        }
      }
    },
    "settings_controls_tvos": {
      "extractionState": "manual",
      "localizations": {
        "ca": {
          "stringUnit": {
            "state": "translated",
            "value": "Llisca a l’esquerra o a la dreta amb el control remot Siri per moure.\nFes clic al botó de reproducció per confirmar."
          }
        },
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Swipe left or right on the Siri Remote to move.\nPress the play button to select."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Desliza a la izquierda o derecha con el mando Siri para mover.\nPulsa el botón de reproducción para seleccionar."
          }
        }
      }
    },
    "settings_controls_watchos": {
      "extractionState": "manual",
      "localizations": {
        "ca": {
          "stringUnit": {
            "state": "translated",
            "value": "Gira la corona digital cap amunt per moure a la dreta i cap avall per moure a l'esquerra. Continua girant per moure't més ràpid. Toca la pantalla o prem el botó lateral per seleccionar."
          }
        },
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Rotate the Digital Crown up to move right and down to move left. Keep turning for faster moves. Tap the screen or press the side button to select."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Gira la corona digital hacia arriba para mover a la derecha y hacia abajo para mover a la izquierda. Sigue girándola para moverte más rápido. Toca la pantalla o pulsa el botón lateral para seleccionar."
          }
        }
      }
    },
    "settings_controls_macos": {
      "extractionState": "manual",
      "localizations": {
        "ca": {
          "stringUnit": {
            "state": "translated",
            "value": "Fes servir les tecles de fletxa esquerra i dreta del teclat per moure.\nClica a la meitat esquerra o dreta de la pantalla amb el trackpad o el ratolí."
          }
        },
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Use the left and right arrow keys on the keyboard to move.\nClick the left or right half of the screen with the trackpad or mouse."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Usa las teclas de flecha izquierda y derecha del teclado para mover.\nHaz clic en la mitad izquierda o derecha de la pantalla con el trackpad o el ratón."
          }
        }
      }
    },
